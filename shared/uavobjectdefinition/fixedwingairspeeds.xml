<xml>
  <object name="FixedWingAirspeeds" settings="true" singleinstance="true">
    <description>Settings for the @ref FixedWingPathFollowerModule</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="manual" period="0"/>
    <telemetrygcs acked="true" updatemode="onchange" period="0"/>
    <telemetryflight acked="true" updatemode="onchange" period="0"/>
    <field defaultvalue="20.0" elements="1" name="AirSpeedMax" type="float" units="m/s">
      <description>Maximum airspeed of the airframe</description>
    </field>
    <field defaultvalue="15.0" elements="1" name="CruiseSpeed" type="float" units="m/s">
      <description>Cruise speed in level flight</description>
    </field>
    <field defaultvalue="11.0" elements="1" name="BestClimbRateSpeed" type="float" units="m/s">
      <description>Airspeed at which airframe climbs the best</description>
    </field>
    <field defaultvalue="8.0" elements="1" name="StallSpeedClean" type="float" units="m/s">
      <description>Stall speed of the airframe</description>
    </field>
    <field defaultvalue="8.0" elements="1" name="StallSpeedDirty" type="float" units="m/s">
      <description>Stall speed of the airframe, with flaps and landing gear deployed</description>
    </field>
    <field defaultvalue="10.0" elements="1" name="VerticalVelMax" type="float" units="m/s">
      <description>Maximum allowed climb or sink rate in guided flight</description>
    </field>
  </object>
</xml>
